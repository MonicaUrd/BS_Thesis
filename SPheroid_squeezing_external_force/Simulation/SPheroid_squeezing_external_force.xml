   <Plugin Name="ExternalPotential">
      
      <!-- External force applied to cell. Each cell type has different force. -->
      <!-- For more flexible specification of the constraint (done in Python) please use ExternalPotentialLocalFlex plugin -->
      <!-- Algorithm options are: PixelBased, CenterOfMassBased -->
      <Algorithm>PixelBased</Algorithm>
      <ExternalPotentialParameters CellType="Periphery" x="10.5" y="0.0" z="0.0"/>
      <ExternalPotentialParameters CellType="Core" x="10.5" y="0.0" z="0.0"/>
   </Plugin>
   
   <Plugin Name="CenterOfMass">
   </Plugin>
   
   <Plugin Name="Contact">
      <!-- Specification of adhesion energies -->
      <Energy Type1="Medium" Type2="Medium">10.0</Energy>
      <Energy Type1="Medium" Type2="Periphery">10.0</Energy>
      <Energy Type1="Medium" Type2="Core">10.0</Energy>
      <Energy Type1="Medium" Type2="Chamber">0.0</Energy>
      <Energy Type1="Periphery" Type2="Periphery">018.0</Energy>
      <Energy Type1="Periphery" Type2="Core">018.0</Energy>
      <Energy Type1="Periphery" Type2="Chamber">050.0</Energy>
      <Energy Type1="Core" Type2="Core">018.0</Energy>
      <Energy Type1="Core" Type2="Chamber">050.0</Energy>
      <Energy Type1="Chamber" Type2="Chamber">0.0</Energy>
      <NeighborOrder>4</NeighborOrder>
   </Plugin>
   
</CompuCell3D>
