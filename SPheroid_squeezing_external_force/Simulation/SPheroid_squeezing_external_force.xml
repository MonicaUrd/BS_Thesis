<CompuCell3D Revision="20200724" Version="4.2.2">
   
   <Metadata>
      
      <!-- Basic properties simulation -->
      <NumberOfProcessors>1</NumberOfProcessors>
      <DebugOutputFrequency>10</DebugOutputFrequency>
      <!-- <NonParallelModule Name="Potts"/> -->
   </Metadata>
   
   <Potts>
      
      <!-- Basic properties of CPM (GGH) algorithm -->
      <Dimensions x="450" y="200" z="1"/>
      <Steps>8000</Steps>
      <Temperature>10.0</Temperature>
      <NeighborOrder>1</NeighborOrder>
   </Potts>
   
   <Plugin Name="CellType">
      
      <!-- Listing all cell types in the simulation -->
      <CellType TypeId="0" TypeName="Medium"/>
      <CellType TypeId="1" TypeName="Periphery"/>
      <CellType TypeId="2" TypeName="Core"/>
      <CellType Freeze="" TypeId="3" TypeName="Chamber"/>
   </Plugin>
<!-- For spheroid size=30    -->
   <Plugin Name="Volume">
      <VolumeEnergyParameters CellType="Periphery" LambdaVolume="5.0" TargetVolume="25"/>
      <VolumeEnergyParameters CellType="Core" LambdaVolume="05.0" TargetVolume="25"/>
      <VolumeEnergyParameters CellType="Chamber" LambdaVolume="5.0" TargetVolume="25"/>
   </Plugin>
   
   <Plugin Name="Surface">
      <SurfaceEnergyParameters CellType="Periphery" LambdaSurface="5.0" TargetSurface="20"/>
      <SurfaceEnergyParameters CellType="Core" LambdaSurface="05.0" TargetSurface="20"/>
      <SurfaceEnergyParameters CellType="Chamber" LambdaSurface="5.0" TargetSurface="20"/>
   </Plugin>
